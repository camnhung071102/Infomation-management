<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="fjs.cs.dto.MSTSTAFFMENU" table="MSTSTAFFMENU">
		<id name="userId" type="string">
			<column name="USERID" />
			<generator class="assigned" />
		</id>
		<property name="screenId" type="string">
			<column name="SCREENID" length="8" />
		</property>
		
	</class>
</hibernate-mapping>